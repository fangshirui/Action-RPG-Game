; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Joystick",
"language": "GDScript",
"path": "res://src/Joystick/Joystick.gd"
}, {
"base": "Node",
"class": "SoundManagerModule",
"language": "GDScript",
"path": "res://addons/sound_manager/module/internal_scripts/SoundManager_config.gd"
} ]
_global_script_class_icons={
"Joystick": "",
"SoundManagerModule": ""
}

[application]

config/name="Action RPG"
run/main_scene="res://src/Levels/Level1.tscn"
boot_splash/image="res://assets/splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/splash.png"

[autoload]

SoundManager="*res://addons/sound_manager/module/SoundManager.tscn"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=640
window/size/test_height=360
window/dpi/allow_hidpi=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "CodeSnippetPopup", "sound_manager" )

